---
import 'src/styles/main.scss';
import {Page} from 'src/components/react/Page';
const { slug } = Astro.params;

const apiKey = import.meta.env.BUILDER_API_PUBLIC_KEY;
const builderModel = import.meta.env.BUILDER_BLOGPOST_MODEL;

---
<html lang="en">
    <head>
        <title></title>
    </head>
    <body>
        <Page apiKey={apiKey} builderModel={builderModel} slug={slug!} client:load></Page>
    </body>
</html>